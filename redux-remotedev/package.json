{
  "name": "redux-remotedev",
  "version": "0.3.0",
  "description": "Redux DevTools for production.",
  "main": "lib/index.js",
  "files": [
    "src",
    "lib",
    "dist",
    "developmentOnly.js",
    "productionOnly.js"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build:lib": "babel src --out-dir lib",
    "build": "npm run build:lib",
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive",
    "prepublish": "npm run lint && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalmoxisus/redux-remotedev.git"
  },
  "keywords": [
    "redux",
    "devtools"
  ],
  "author": "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zalmoxisus/redux-remotedev/issues"
  },
  "homepage": "https://github.com/zalmoxisus/redux-remotedev",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "isparta": "^4.0.0",
    "mocha": "^3.0.2",
    "redux": "^3.5.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "jsan": "^3.1.3",
    "lodash": "^4.0.0",
    "remotedev-utils": "^0.0.5"
  }
}
